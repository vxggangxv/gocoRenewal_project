<% include ../parts/_docHead.ejs %>
<head>
    <!-- <%= initPage = '/mypage/' %> -->
    <% include ../parts/head/_headMeta.ejs %>
    <link rel="stylesheet" href="http://static.go.co.kr/css/renew/libs/jquery.bxslider.css">
    <link rel="stylesheet" href="http://static.go.co.kr/css/renew/libs/prettyPhoto.css">
    <% include ../parts/head/_commonCss.ejs %>
    <% include ../parts/head/_headScript.ejs %>
    <script src="http://static.go.co.kr/js/renew/libs/jquery.bxslider.min.js"></script>
    <script src="http://static.go.co.kr/js/renew/libs/jquery.prettyPhoto.js"></script>
    <% include ../parts/head/_commonScript.ejs %>
</head>
<body>
    <h1 class="hidden">고코투어</h1>
    <div class="rn-gwrap">
        <% include ../parts/_header.ejs %>

        <!--백드롭, 달력 클릭시 뒤에 전체 배경-->
		<div id="backDrop"></div>

        <!--전체 콘텐츠 영역-->
		<div class="rn-contents">
            <div class="rn-container rn-container02">
                <aside class="lab bsLsbo">
                    <h2>마이페이지</h2>
                    <nav class="lnb">
                        <ul>
                            <li class="on"><a href="">예약 내역 조회</a></li>
                            <li><a href="">쿠폰 조회</a></li>
                            <li><a href="">적립금 조회</a></li>
                            <li><a href="">찜 목록</a></li>
                            <li><a href="">나의 이용후기</a></li>
                            <li><a href="">나의 문의내역</a></li>
                            <li><a href="">무료숙박권 예약</a></li>
                            <li class="type02"><a href="">회원 정보관리</a></li>
                            <li class="type02"><a href="">개인 결제창</a></li>
                        </ul>
                    </nav>

                    <div class="cus-call">
                        <a href="#"><img src="http://img.go.co.kr/i_faqPh.png" alt="고객센터전화문의"> 고객센터 전화문의</a>
                    </div>
                </aside>

                <div class="main main-mypage bsLs">
                    <p class="row-name">
                        <span class="name">김지영(jiyoungkim)님, 안녕하세요!</span>
                        <span class="s-r">
                            <span class="point cf">
                                <span class="txt">MY POINT</span>
                                <span class="num"><span class="colr-org f-bdr">25,222</span>P</span>
                            </span>
                            <span class="pieces">
                                <span class="coupon-per">
                                    <span class="square01"></span><span class="square02">%</span>
                                    <span class="sunV">
                                            <span class="won"></span>
                                            <span class="won"></span>
                                            <span class="won"></span>
                                            <span class="won"></span>
                                    </span>
                                </span>
                                <span class="num f-bd"><span class="colr-org f-bdr">6</span>장</span>
                            </span>
                        </span>
                    </p>

                    <section class="sec-cmm cf sec-bkHistory">
                        <h2>예약내역 조회</h2>
                        <ul class="row-tab cf">
                            <li class="tab-1 on">이용가능</li>
                            <li class="tab-2">이용완료</li>
                            <li class="tab-3">취소/환불</li>
                        </ul>
                        <table class="tb-bk">
                            <colgroup>
                                <col class="nth-1">
                                <col class="nth-2">
                                <col class="nth-3">
                                <col class="nth-4">
                                <col class="nth-5">
                                <col class="nth-6">
                            </colgroup>
                            <thead>
                                <tr>
                                    <th class="nth-1">예약처</th>
                                    <th class="nth-2">접수일자</th>
                                    <th class="nth-3">숙소명</th>
                                    <th class="nth-4">예약상태</th>
                                    <th class="nth-5">변경/취소</th>
                                    <th class="nth-6">출력</th>
                                </tr>
                            </thead>
                            <tbody>
                                <!-- 내용 없을 경우 -->
                                <tr class="nothing">
                                    <td colspan="6">
                                        <p>예약/구매 내용이 없습니다.</p>
                                        <p class="row-btn"><a href="" class="a-btn">구매하신 예약건이 보이지 않는다면, 여기를 클릭하세요. <span class="ic-rArrow rArrow"></span></a></p>
                                    </td>
                                </tr>
                            </tbody>
                        </table>

                        <div class="pagenation" id="pagenation">
                            <a class="pprev" href="javascript:;"><span class="ic-lArrow"></span><span class="ic-lArrow"></span></a>
                            <a class="pprev" href="javascript:;"><span class="ic-lArrow"></span></a>
                            <a class="active" href="javascript:;">1</a>
                            <a href="javascript:;">2</a>
                            <a href="javascript:;">3</a>
                            <a href="javascript:;">4</a>
                            <a href="javascript:;">5</a>
                            <a class="nnext" href="javascript:;"><span class="ic-rArrow"></span></a>
                            <a class="nnext" href="javascript:;"><span class="ic-rArrow"></span><span class="ic-rArrow"></span></a>
                        </div>
                    </section>

                    <section class="sec-cmm cf sec-faq">
                        <div class="row-h cf">
                            <h2 class="pull-left;">FAQ 자주묻는 질문 <span class="sm">여행에 대한 궁금증을 찾아보세요</span></h2>
                            <span class="sch-box cf pull-right">
        						<input name="keyword" class="in" type="text" value="" placeholder="무엇을 도와드릴까요?" onkeypress="if(event.keyCode == 13) { search_faq(); return false; }">
        						<a href="javascript:search_faq();" class="i-mag">검색</a>
        					</span>
                        </div>

                        <!-- faq 인클루드 -->
                        <% include ../parts/mypage/_faq.ejs %>
                    </section>
                </div>
                <!-- main end -->

            </div>
        </div>

        <% include ../parts/_footer.ejs %>
    </div>
</body>
