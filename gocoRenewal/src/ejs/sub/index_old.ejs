		if($("#product_selected").css("display") == "none"){
			$("#product_selected").show();
		} else{
			$("#product_selected").hide();
			utils.basic_l_on('product_selected');
		}
	});

	$("input[name='category1']").click(function(){
		$("#contents_save").val("");
		$("#contents").val("");
	});

	$("#contents").focusin(function() {
		if($("input[name='category1']").is(":checked") == true){
			//변수
			if($("#_type3").is(":checked") == true){
			var contents_txt = txt1;
			}else{
			var contents_txt = txt2;
			}
			var contents_save = $("#contents_save"); //수장내용 저장공간
			var dt = $("#contents"); // 내용
			//변수끝


			if(dt.val() == ""){
				dt.val(contents_txt);
			}else if(dt.val() != txt1 && dt.val() != txt2){
				dt.val(contents_save.val());
			}

		}else{
			alert("분류를 선택해주세요.");
			$("#contents").blur();
		}
	});

	$("#contents").focusout(function() {
		if($("input[name='category1']").is(":checked") == true){
			//변수
			if($("#_type3").is(":checked") == true){
			var contents_txt = txt1;
			}else{
			var contents_txt = txt2;
			}
			var contents_save = $("#contents_save");
			var dt = $("#contents"); // 내용
			//변수끝


			if(contents_txt == dt.val()){
				dt.val("");
			}else{
				contents_save.val(dt.val());
			}
		}
	});

	$(document).on("keyup", "input:text[numberOnly]", function() {$(this).val( $(this).val().replace(/[^0-9]/gi,"") );});
});


